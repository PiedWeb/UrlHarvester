{
  "name": "piedweb/url-harvester",
  "type": "library",
  "description": "Harvest statistics and meta data from an URL or his source code (seo oriented).",
  "keywords": [
    "Pied Web",
    "Url Meta Data Harvester",
    "crawl",
    "seo",
    "meta data",
    "url",
    "canonical"
  ],
  "homepage": "https://dev.piedweb.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Robin@PiedWeb",
      "email": "contact@piedweb.com",
      "homepage": "https://piedweb.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "neitanod/forceutf8": "^2.0.4",
    "piedweb/text-analyzer": "^0.0.4",
    "spatie/robots-txt": "^1.0.10|^2",
    "piedweb/curl": "^0.0.18",
    "symfony/dom-crawler": "^5.2",
    "symfony/css-selector": "^5.2",
    "league/uri": "^6.5",
    "jeremykendall/php-domain-parser": "^6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.12",
    "symfony/var-dumper": "^5.3",
    "friendsofphp/php-cs-fixer": "^3.2"
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix"
  },
  "autoload": {
    "psr-4": {
      "PiedWeb\\UrlHarvester\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PiedWeb\\UrlHarvester\\": "tests"
    }
  }
}
